<?xml version="1.0" encoding="UTF-8"?>
<theme>
   <formatVersion>7</formatVersion>

   <view name="splash">
      <image name="background">
         <path>./asset/images/splash.svg</path>
         <origin>0 0</origin>
         <size>1 1</size>
         <pos>0 0</pos>
      </image>
      <image name="progressbar">
         <color>00000000</color>
         <colorEnd>00000000</colorEnd>
         <roundCorners>0</roundCorners>
      </image>
      <image name="progressbar:active">
         <color>FF0000</color>
         <colorEnd>FF0000</colorEnd>
      </image>
      <text name="label">
         <color>FFFFFF00</color>
      </text>
   </view>
</theme>
<theme>
   <formatVersion>7</formatVersion>

   <feature supported="carousel">
      <view name="system">
         <image name="bg_game_count" extra="true">
            <path>./asset/images/game_count_bg.svg</path>
            <size>0.375 0.125</size><!-- 240, 60 -->
            <origin>1 0</origin>
            <pos>1 0.475</pos>
			<storyboard>
			  <animation property="x" from="1" to="0.625" begin="120" duration="120" mode="Linear"/>
			</storyboard>
            <zIndex>6</zIndex>
         </image>
         <text name="systemInfo">
            <size>0.3 0.1</size>
            <pos>0.70 0.473</pos>
            <color>FFFFFF</color>
            <alignment>center</alignment>
            <fontSize>0.030</fontSize>
            <rotation>4</rotation>
            <zIndex>7</zIndex>
         </text>
      </view>
   </feature>
</theme>